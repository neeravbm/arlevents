#tabs-titles{
  float:left;
  margin-right:40px;
  background: #203B6A;
  width: 17%;
}

.front #tabs-titles{
  float:left;
  margin-right:40px;
  background: #203B6A;
  width: 12%;
}

#tabs-titles div{
  cursor:pointer;
  color:#fff;
  height: 40px;
  padding: 10px;
  border-bottom: 1px solid #FFFFFF;
  font-weight:bolder;
  background:#4D7BBA;
}

#tabs-titles div.current{
  background:url('../images/leftblueselectd_arrow.png')  no-repeat right center;
  margin: 0 0 0 5%;
  width: 86%;
}


#tabs-titles div a {
  color:#fff;
  text-decoration:none;
}


#tabs-contents{
    background:#F2F2F2;
    margin-left:100px;
    padding:5px;
}

.front #tabs-contents{
    background:#FFFFFF;
    margin-left:100px;
    padding:5px;
}

#tabs-contents div{
    display:none;
}
#tabs-contents div:first-child{
    display:block;
}
#tabs-contents table {
    border: none;
    width: 89%;
}
#tabs-contents .block_title {
    font-size: 20px;
    font-weight: normal;
    margin-bottom: 0;
    padding: 10px 0;
    color:#3B3B3B;
}

#tabs-contents table tr td{
    color:#3D3D3D;
}

#tabs-contents table tr.even td {
    background:#F2F2F2;
}

#tabs-contents table tr.even, #tabs-contents table tr.odd {
    border-bottom:none;
}

