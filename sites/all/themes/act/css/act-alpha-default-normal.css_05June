/************* Tabs by devendra************/

.ui-tabs {
	zoom: 1;
}

.ui-tabs .ui-tabs-nav {
	list-style: none;
	position: relative;
	padding: 0;
	margin: 50px 0 0;
	background:#203b6a;
	width:100%;
	float:left;
	height:40px;
}

.ui-tabs .ui-tabs-nav div {
	position: relative;
	float: left;
	margin: 0;
	padding: 0;

}

.ui-tabs .ui-tabs-nav div a {
	display: block;
	padding: 10px 20px;
    border-bottom-color: #ccc;
	outline: none;
	background: #203B6A;
	border-right: 2px #162742 solid;
    color:#fff;
	text-decoration:none;
}

.ui-tabs .ui-tabs-nav div a:hover, 
.ui-tabs .ui-tabs-nav div.active a {
    background: #162742;
}

.ui-tabs .ui-tabs-nav div.active {
     background:url('../images/arrow.png') no-repeat center bottom;
     padding-bottom:7px;
}

.ui-tabs .ui-tabs-nav div.ui-tabs-selected a {
	padding: 10px 20px 12px 20px;
	background: #fff;
	border-bottom-style: none;
}
.ui-tabs .ui-tabs-nav div.ui-tabs-selected a,
.ui-tabs .ui-tabs-nav div.ui-state-disabled a,
.ui-tabs .ui-tabs-nav div.ui-state-processing a {
	cursor: default;
}
.ui-tabs .ui-tabs-nav div a,
.ui-tabs.ui-tabs-collapsible .ui-tabs-nav div.ui-tabs-selected a {
	cursor: pointer;
}

.ui-tabs .tabcontent{
	display: block;
	clear: both;
    padding: 10px 0;
}

.ui-tabs .ui-tabs-hide {
	display: none;
}


