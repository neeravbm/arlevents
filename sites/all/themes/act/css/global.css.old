/********** Upper Drupal Tabs ********/

.tabs ul.primary {
    height:23px;
    background:#D3CECE;
    padding: 6px 0 0;
    border-left: 4px solid #D3CECE;
    border-top: 4px solid #D3CECE;
}

.tabs ul li {
    margin-right : 0px;
}

.tabs ul.primary li a.active {
    background:#850606;
    color:#fff;
}

.tabs ul.primary li a {
    margin-right:0px;
    background:#EEEBEB;
    border-right:5px solid #D3CECE;
    color:#626060;
    padding-bottom:4px;
    padding-top:5px;
    border-bottom:none;
    border-top:none;
    border-left:none;
}

.tabs ul li.last{
  float:right;
  margin-top:0;
}

@-moz-document url-prefix() {
 .tabs ul li.last{
   float:right;
   margin-top:-17px;
   padding-bottom:2px;
 }
 
 .tabs ul li.last a{
   padding-bottom:2px;
 }
}

